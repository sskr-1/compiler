// Loop examples
sumToN(n) {
    var sum = 0;
    var i = 1;
    
    while (i <= n) {
        sum = sum + i;
        i = i + 1;
    }
    
    return sum;
}

power(base, exp) {
    var result = 1;
    var i = 0;
    
    while (i < exp) {
        result = result * base;
        i = i + 1;
    }
    
    return result;
}

main() {
    var n = 10;
    var sum = sumToN(n);
    var pow = power(2, 3);
    return sum + pow;
}